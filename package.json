    "@storybook/addon-actions": "^6.0.10",
    "@storybook/addon-essentials": "^6.0.10",
    "@storybook/addon-links": "^6.0.10",
    "@storybook/addon-notes": "^5.3.19",
    "@storybook/html": "^6.0.10",
    "babel-loader": "^8.1.0",
    "copy-webpack-plugin": "^6.0.3",
    "react-is": "^16.13.1",
    "write-file-webpack-plugin": "^4.5.1"
  },
  "license": "MIT",
  "dependencies": {}
